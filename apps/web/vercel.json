{
    "buildCommand": "npm run build",
    "devCommand": "npm run dev",
    "env": {
      "NEXT_PUBLIC_API_URL": "@next_public_api_url",
      "NEXT_PUBLIC_SENTRY_DSN": "@next_public_sentry_dsn"
    },
    "rewrites": [
      {
        "source": "/api/:path*",
        "destination": "https://<SEU_BACKEND_URL>/:path*"
      }
    ]
  }
  